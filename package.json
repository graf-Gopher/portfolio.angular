{
  "name": "portfolio.angular",
  "title": "Developer Portfolio",
  "version": "0.0.1-dev.11",
  "description": "Portfolio for developers",
  "author": {
    "name": "Under Tree Entertainment",
    "url": "https://under-tree-e.com",
    "email": "under-tree-e@outlook.com",
    "twitter": "under_tree_e",
    "linkedin": "under-tree-e"
  },
  "homepage": "https://xxx",
  "demopage": "https://xxx",
  "repository": {
    "type": "git",
    "url": "https://github.com/graf-Gopher/portfolio.angular"
  },
  "bugs": {
    "url": "https://github.com/graf-Gopher/portfolio.angular/issues"
  },
  "keywords": [
    "typescript",
    "angular"
  ],
  "private": true,
  "license": "Open-Source/2025",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "build": "ng build",
    "ssr": "ng run under_tree.angular:ssr",
    "server": "node dist/under_tree.angular/server/main.js",
    "sonar:scan": "docker run -it --network=databases_default --rm -v $(pwd):/usr/src sonarsource/sonar-scanner-cli",
    "---": "INSTALL",
    "init": "npm install ts-node --save-dev && npx ts-node node_scripts --init",
    "load": "npm i --force",
    "fix": "npm audit fix --force",
    "rebuild": "npm rebuild",
    "----": "NODE SCRIPTS",
    "ns-scan": "npx ts-node node_scripts --scan",
    "ns-help": "npx ts-node node_scripts --help",
    "ns-build": "npx ts-node node_scripts --build",
    "ns-publish": "npx ts-node node_scripts --publish",
    "git-publish": "angular-cli-ghpages --repo=https://github.com/graf-Gopher/portfolio.angular.git --dir=dist/portfolio.angular --no-silent",
    "-----": "VERSIONS",
    "major-update": "npm version major --no-git-tag-version",
    "minor-update": "npm version minor --no-git-tag-version",
    "patch-update": "npm version patch --no-git-tag-version",
    "dev-update": "npm version prerelease --preid=dev --no-git-tag-version && git add . && git commit -m 'dev_update_'$npm_package_version && git push --all",
    "------": "LOCAL MODULES",
    "core-local": "npm i ../../addons/ute-core.ngx/dist/ngx-ute-core && node ../../addons/ute-core.ngx/install-peers.js",
    "core-npm": "npm r ngx-ute-core && node ../../addons/ute-core.ngx/remove-peers.js && npm i ngx-ute-core"
  },
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^19.1.3",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/platform-server": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@angular/ssr": "^19.1.6",
    "keen-slider": "^6.8.6",
    "ngx-lightbox": "^3.0.0",
    "ngx-ute-core": "file:../../addons/ute-core.ngx/dist/ngx-ute-core",
    "npm-check-updates": "^17.1.14",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.0",
    "@capacitor/preferences": "^7.0.0",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.13.1",
    "bestzip": "^2.2.1",
    "canvas": "^3.1.0",
    "clear-folder": "^5.1.0",
    "compressorjs": "^1.2.1",
    "crypto-ts": "^1.0.2",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-cookie-service": "^19.1.0",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0"
  }
}
